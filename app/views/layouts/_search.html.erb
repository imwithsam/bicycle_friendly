<%= form_tag("/search", method: :get, class: "form-inline") %>
<%= text_field_tag "address", nil, class: "form-control",
  size: "30",
  placeholder: address || "Where are you walking from?" %>
<%= submit_tag "Walk", class: "btn btn-primary" %><br>
<%= text_field_tag "term", nil, class: "form-control",
  size: "30",
  placeholder: term || "Find" %>
